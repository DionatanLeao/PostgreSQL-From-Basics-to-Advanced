SELECT * FROM AREA;
SHOW TABLES;
SELECT * FROM FUN;
SELECT DEPTNUM, CARGO, SALARIO FROM FUN;
SELECT * FROM  AREA WHERE DEPTNUM = 10;
SELECT * FROM  FUN WHERE DEPTNUM = 10;
SELECT * FROM  FUN WHERE DEPTNUM = 10 OR COMISSAO IS NOT NULL OR SALARIO <= 2000 AND DEPTNUM = 20;
SELECT * FROM  FUN WHERE (DEPTNUM = 10 OR COMISSAO IS NOT NULL OR SALARIO <= 2000) AND DEPTNUM = 20;